#include <stdio.h>

int main(){
int s = 0;
int b = 0; 
int m = 0;
int n = 5;
int c[] = {1,2,3,4,5};


for (int i = 0; i < n; i++) {
    for (int j = i + 1;j < n; j++) {
        if (c[i] == c[j]) {
            m++;
        }
    }
    // Проверяем, больше ли количество повторяющихся элементов, чем значение s
    if (m > s) {
        s = m;
        b = i;
    }
}

if (s > 0) {
    printf("Элемент %d повторяется больше всего раз (%d раз)", c[b], s);
} else {
    printf("В массиве нет повторяющихся элементов");
}
}
